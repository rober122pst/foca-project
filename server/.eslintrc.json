{
    "extends": [
        "@rocketseat/eslint-config/node",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", {
            "tabWidth": 4,
            "printWidth": 80
        }]
    }
}